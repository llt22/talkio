{
  "identifier": "default",
  "description": "Talkio default permissions",
  "windows": ["main"],
  "webviews": ["main"],
  "permissions": [
    "core:default",
    "sql:default",
    "sql:allow-load",
    "sql:allow-execute",
    "sql:allow-select",
    "sql:allow-close",
    "fs:default",
    "notification:default",
    "opener:default",
    "dialog:default",
    "fs:allow-write-text-file",
    "http:default",
    {
      "identifier": "http:allow-fetch",
      "allow": [{ "url": "http://**" }, { "url": "https://**" }]
    }
  ]
}
